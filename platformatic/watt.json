{
  "$schema": "https://schemas.platformatic.dev/@platformatic/runtime/2.73.1.json",
  "watch": true,
  "autoload": {
    "path": "web",
    "exclude": [
      "docs"
    ]
  },
  "logger": {
    "level": "{PLT_SERVER_LOGGER_LEVEL}"
  },
  "server": {
    "hostname": "{PLT_SERVER_HOSTNAME}",
    "port": "{PORT}"
  },
  "managementApi": "{PLT_MANAGEMENT_API}",
  "entrypoint": "composer",
  "web": [
    {
      "id": "oop",
      "path": "{PLT_SERVICE_OOP_PATH}",
      "url": "http://localhost:5173/"
    }
  ]
}